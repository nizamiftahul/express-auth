  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.9.13",
    "speakeasy": "^2.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^16.18.98",
    "@types/nodemailer": "^6.4.15",
    "@types/speakeasy": "^2.0.10",
    "nodemon": "^3.1.3",
    "prisma": "^4.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^1.1.8",
